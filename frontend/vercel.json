{
  "version": 2,
  "rewrites": [
    {
      "source": "/Bag.html",
      "destination": "/frontend/public/Bag.html"
    },
    {
      "source": "/Blog.html",
      "destination": "/frontend/public/Blog.html"
    },
    {
      "source": "/PersonalAccount.html", 
      "destination": "/frontend/public/PersonalAccount.html"
    },
    {
      "source": "/ProductCatalog.html",
      "destination": "/frontend/public/ProductCatalog.html"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/public/main.html"
    }
  ]
}